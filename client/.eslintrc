{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
		"semi": [0],
        "indent": [1, 4],
        "react/jsx-indent-props": [1, 4],
		"react/jsx-filename-extension": [0],
		"react/jsx-indent": [0],
        "no-tabs": [0],
        "no-trailing-spaces": [0],
        "react/jsx-one-expression-per-line": [0],
        "react/prop-types": [0],
        "no-console": [0],
        "func-names": [0],
        "no-debugger": [0],
        "comma-dangle": [0],
        "dot-notation": [0],
        "arrow-parens": [0],
        "no-param-reassign": [0],
        "arrow-body-style": [0],
        "react/sort-comp": [0],
        "react/destructuring-assignment": [0],
        "react/no-danger": [0],
        "max-len": [0, {"code": 150}]
    }
}
